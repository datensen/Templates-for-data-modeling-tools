{{#*inline "queryDetail"}}
  {{~#if queryObject.generate~}}
    {{~> commentDetail object=queryObject~}}
    {{~#isValueDefined queryObject.directive}} {{queryObject.directive}} {{/isValueDefined~}} 
    {{~#isValueDefined queryObject.code~}}
      {{{queryObject.code}}}
